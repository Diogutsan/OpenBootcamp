		numero = 0;
        
        funcionIf();
    
    
	}
    public static void funcionIf(numero)  {
        if numero <0;
            System.out.println(numero, 'Este numero es negativo');
        else;
            System.out.println(numero, 'Este numero es positivo');
    }
    public static void funcionWhile(numero) {
        
        while (numero < 3) {
        
            System.out.println(numero);
            numero = numero += 1;
        }
    }
    public static void funcionDo(numero) { 
  
        do{
		    System.out.println(numero);
            numero = nÃºmero += 1;
  
        }while (numeroWhile < 3); 
  
    }
